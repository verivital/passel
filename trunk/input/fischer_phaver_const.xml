			<stop equn='x[i] &gt;= a' />
			<dai variable='x[i]' equn='x[i]_dot &gt;= c and x[i]_dot &lt;= d'/>
		</mode>
		
		<mode id='3' name='check' initial='False'>
			<dai variable='x[i]' equn='x[i]_dot &gt;= c and x[i]_dot &lt;= d'/>
		</mode>
		
		<mode id='4' name='cs' initial='False'>
			<dai variable='x[i]' equn='x[i]_dot = 0.0'/>
		</mode>
<!--
		<mode id='5' name='reset' initial='False'>
